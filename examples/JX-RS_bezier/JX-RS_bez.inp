  check_curvature = .true.
  max_curv_reverse_top = 1
  curv_top_spec%max_te_curvature = 2
/


&geometry_targets
  ngeo_targets    = 2					 
  target_type(1)  = 'Thickness'
  target_geo(1)   = 0.074
  target_type(2)  = 'Camber'
  target_geo(2)   = 0.013
/  


&panelling_options
  npan = 140
  le_bunch = 0.90
  te_bunch = 0.50
/


