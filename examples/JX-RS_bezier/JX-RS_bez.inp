  dynamic_weighting = .true. 
  allow_improved_target = .true. 

  noppoint         = 8 
  
  re_default       = 250000				 					

! ---------------------  Type 1 Default ---------------

  op_point(1) = -0.14                     
  optimization_type(1) = 'target-drag'
  target_value(1) = 0.00825
  weighting(1) =  -0.5					   
					   
  op_point(2) = -0.05	
  optimization_type(2) = 'target-drag'
  target_value(2) = 0.00706  

  op_point(3) = 0.20
